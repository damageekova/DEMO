#include <iostream>
#include <math.h>
using namespace std;
/*Напишите эффективную по памяти программу, выписывающую первые N составных чисел.*/

int main (){
int n;
cout<<"Enter n  ";
cin>>n;
int h=1;
int k=0;
while(k<n){
for(int i=2;i<h;i++){
        if(h%i==0) { k++;cout<<h<<" "<<endl;break;
        }
 }
 h++;
}
return 0;
}
